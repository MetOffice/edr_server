{
  "links": [
    {
      "href": "{{ links_href }}",
      "hreflang": "en",
      "rel": "self",
      "type": "application/json"
    }
  ],
  {% set file_count = len(collection_files) %}
  "collections": [
  {% for i, filename in enumerate(collection_files, 1) %}
    {% set collection_filename = "{}".format(filename) %}
    {% module Template(collection_filename) %}{% if i < file_count %},{% end %}
  {% end %}
  ]
}