{% whitespace oneline %}
"{{ param.name }}": [
  {
    "type": "Parameter",
    "id": "{{ param.id }}",
    "description": "{{ param.description }}",
    "unit": {
      "label": "{{ param.unit_label }}",
      "symbol": {
        "value": "{{ param.unit_value }}",
        "type": "{{ param.unit_defn }}"
      }
    },
    "observedProperty": {
      "id": "{{ param.property_id }}",
      "label": "{{ param.property_label }}"
    }
  }
]