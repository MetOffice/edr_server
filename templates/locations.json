{% whitespace oneline %}
{
  "type": "FeatureCollection",
  {% set bbox_point_ct = len(collection_bbox) %}
  "bbox": [
    {% for i, value in enumerate(collection_bbox, 1) %}
    "{{ value }}"{% if i < bbox_point_ct %},{% end %}
    {% end %}
  ],
  {% set feature_count = len(locations) %}
  "features": [
    {% for i, location in enumerate(locations, 1) %}
    {% module Template("locations_feature.json", location=location) %}{% if i < feature_count %},{% end %}
    {% end %}
  ]
}