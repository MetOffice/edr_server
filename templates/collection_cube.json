{% whitespace oneline %}
"cube": {
  "link": {
    "href": "http://data.example.com/collections/monitoringsites/locations/1234",
    "hreflang": "en",
    "rel": "data",
    "type": "application/geo+json",
    "title": "{{ collection.name }}",
    "templated": true,
    "variables": {
      "title": "Cube query",
      "description": "Query to return a data cube",
      "query_type": "cube",
      "height_units": [
        "m",
        "hPa"
      ],
      "output_formats": [
        "CoverageJSON",
        "GeoJSON"
      ],
      "default_output_format": "CoverageJSON",
      "crs_details": [
        {
          "crs": "{{ collection.crs_name }}",
          "wkt": "{{ collection.crs }}"
        }
      ]
    }
  }
},